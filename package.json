{
  "name": "meteor_vuetify",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "test": "TEST_WATCH=1  meteor test --driver-package meteortesting:mocha",
    "test-coverage": "BABEL_ENV=COVERAGE TEST_BROWSER_DRIVER=puppeteer COVERAGE=1 COVERAGE_OUT_HTML=1 COVERAGE_APP_FOLDER=$PWD/ TEST_WATCH=1  meteor test --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "createApi": "node ./exec/api/createApi.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "lodash": "^4.17.20",
    "meteor-node-stubs": "^1.0.1",
    "simpl-schema": "^1.10.2",
    "vue": "^2.6.12",
    "vue-meteor-tracker": "^2.0.0-beta.5",
    "vue-router": "^3.4.9",
    "vuetify": "^2.4.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "devDependencies": {
    "@mdi/font": "^5.9.55",
    "jquery": "^3.5.1",
    "puppeteer": "^5.5.0"
  }
}
